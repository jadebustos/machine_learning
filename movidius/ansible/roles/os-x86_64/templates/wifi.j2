[connection]
id={{ ssid }}
uuid={{ uuid }}
type=wifi
permissions=
secondaries=
timestamp=1588154696

[wifi]
mac-address={{ mac }}
mac-address-blacklist=
mac-address-randomization=0
mode=infrastructure
seen-bssids={{ bssids }};
ssid={{ ssid }}

[wifi-security]
group=
key-mgmt={{ key_mgmt }}
pairwise=
proto=
psk={{ psk }}

[ipv4]
address1={{ ip_address }}/{{ netmask }},{{ routers }}
dns={{ dns }};
dns-search=
method=manual

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
ip6-privacy=0
method=auto
